//create an array of 5 elements and print the values.class
//local variables
//instance variable
//static variable
import java.util.Scanner;

public class EFArrayDemo {
   // int arr[]= new int[5];
    int arr[];
    EFArrayDemo()
    {
        arr = new int[5];
        Scanner s = new Scanner(System.in);
        for(int i=0;i<arr.length;i++){
            arr[i]=s.nextInt();
        }
    }


    public static void main(String[] args) {
        EFArrayDemo obj1 = new EFArrayDemo();
        for (int i = 0; i < obj1.arr.length; i++) {
            System.out.println(obj1.arr[i]);
        }
    }
}
